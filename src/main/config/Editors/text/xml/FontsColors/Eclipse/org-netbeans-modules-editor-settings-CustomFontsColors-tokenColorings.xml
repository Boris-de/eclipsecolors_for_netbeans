<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor default="error" name="xml-error"/>
    <fontcolor default="default" foreColor="ff2a00ff" name="xml-value">
        <font style="italic"/>
    </fontcolor>
    <fontcolor default="default" foreColor="ff008080" name="xml-pi-target"/>
    <fontcolor default="default" foreColor="ff008080" name="xml-pi-end">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="markup-element" foreColor="ff3f7f7f" name="xml-tag">
        <font style="plain"/>
    </fontcolor>
    <fontcolor default="whitespace" name="xml-EOL"/>
    <fontcolor default="markup-attribute" foreColor="ff7f007f" name="xml-attribute"/>
    <fontcolor default="default" name="xml-text"/>
    <fontcolor default="operator" name="xml-operator"/>
    <fontcolor default="default" name="xml-pi-content"/>
    <fontcolor default="default" name="xml-cdata-section"/>
    <fontcolor default="whitespace" name="xml-ws"/>
    <fontcolor default="default" foreColor="ff008080" name="xml-pi-start"/>
    <fontcolor default="comment" name="xml-comment"/>
    <fontcolor default="entity-reference" name="xml-ref"/>
    <fontcolor default="default" foreColor="ff008080" name="xml-doctype"/>
</fontscolors>
